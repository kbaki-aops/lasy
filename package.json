{
  "name": "lasy",
  "displayName": "Lasy",
  "description": "I'm a VS Code extension that compiles Asymptote files to SVG format and displays the output.",
  "version": "0.0.8",
  "publisher": "kbaki-aops",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/kbaki-aops/lasy.git"
  },
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Lasy",
      "properties": {
        "lasy.asyPath": {
          "type": "string",
          "default": "asy",
          "description": "Lasy's path to the asy binary."
        },
        "lasy.png.if": {
          "type": "boolean",
          "default": false,
          "description": "EXPERIMENTAL, uses GhostScript: If Lasy should rasterise to PNG."
        },
        "lasy.png.dpi": {
          "type": "number",
          "default": 720,
          "description": "EXPERIMENTAL, uses GhostScript: The DPI of the PNG."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "~22.17.2",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2"
  }
}
